{
  "name": "libbash",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
    "ghcr.io/georglauterbach/dev-container-features/vscode-cache-extensions:0.1": {},
    "ghcr.io/georglauterbach/dev-container-features/hermes:11.2.3": {},
    "ghcr.io/georglauterbach/dev-container-features/lang-bash:0.1.1": {}
  },
  "containerUser": "ubuntu",
  "remoteUser": "ubuntu",
  "updateRemoteUserUID": true,
  "postCreateCommand": "hermes",
  "customizations": {
    "vscode": {
      "extensions": [
        "jetmartin.bats",
        "streetsidesoftware.code-spell-checker"
      ]
    }
  }
}
